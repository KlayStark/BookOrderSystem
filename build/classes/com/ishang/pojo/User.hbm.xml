<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ishang.pojo.User" table="User">
        <id name="id" type="int">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="Sid" type="java.lang.String">
            <column name="Sid" />
        </property>
        <property name="Type" type="java.lang.String">
            <column name="Type" />
        </property>
        <property name="Sname" type="java.lang.String">
            <column name="Sname" />
        </property>
        <property name="Spassword" type="java.lang.String">
            <column name="Spassword" />
        </property>
        <property name="Phone" type="java.lang.String">
            <column name="Phone" />
        </property>
        <property name="Grade" type="java.lang.String">
            <column name="Grade" />
        </property>
        <property name="Major" type="java.lang.String">
            <column name="Major" />
        </property>
        <property name="Semail" type="java.lang.String">
            <column name="Semail" />
        </property>
    </class>
</hibernate-mapping>
