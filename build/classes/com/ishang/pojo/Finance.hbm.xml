<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.ishang.pojo.Finance" table="Finance">
  <id name="Borderid" type="int">
   <column name="Borderid"/>
   <generator class="assigned"/>
  </id>
  <property name="Apay" type="float">
   <column name="Apay"/>
  </property>
  <property name="Gdate" type="java.util.Date">
   <column name="Gdate"/>
  </property>
  <property name="Pdate" type="java.util.Date">
   <column name="Pdate"/>
  </property>
  <property name="Ptprice" type="float">
   <column name="Ptprice"/>
  </property>
  <property name="Pid" type="int">
   <column name="Pid"/>
  </property>
 </class>
</hibernate-mapping>
