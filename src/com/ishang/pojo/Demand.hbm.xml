<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.ishang.pojo.Demand" table="Demand">
  <id name="Bid" type="int">
   <column name="Bid"/>
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="Bname" type="java.lang.String">
   <column name="Bname"/>
  </property>
  <property generated="never" lazy="false" name="Bnum" type="int">
   <column name="Bnum"/>
  </property>
 </class>
</hibernate-mapping>
